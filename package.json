{
    "name": "churchconnect-backend",
    "version": "0.0.1",
    "description": "",
    "author": "Blossom Obasi <blossomobasi2@gmail.com>",
    "private": true,
    "license": "MIT",
    "scripts": {
        "preinstall": "npx only-allow yarn",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "generate": "npx prisma generate",
        "migrate": "npx prisma migrate deploy",
        "migrations": "npx prisma migrate dev --create-only --name",
        "migrate:dev": "npx prisma migrate dev"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.919.0",
        "@aws-sdk/client-ses": "^3.919.0",
        "@aws-sdk/cloudfront-signer": "^3.916.0",
        "@aws-sdk/lib-storage": "^3.919.0",
        "@aws-sdk/s3-request-presigner": "^3.919.0",
        "@keyv/redis": "^5.1.3",
        "@nestjs-modules/mailer": "^2.0.2",
        "@nestjs/bull": "^11.0.4",
        "@nestjs/cache-manager": "^3.0.1",
        "@nestjs/common": "^10.0.0",
        "@nestjs/config": "^4.0.2",
        "@nestjs/core": "^10.0.0",
        "@nestjs/jwt": "^11.0.1",
        "@nestjs/mapped-types": "*",
        "@nestjs/passport": "^11.0.5",
        "@nestjs/platform-express": "^10.0.0",
        "@nestjs/swagger": "^11.2.1",
        "@prisma/client": "^6.18.0",
        "@react-email/components": "^0.5.7",
        "@react-email/render": "^1.4.0",
        "@sentry/nestjs": "^10.22.0",
        "@sentry/profiling-node": "^10.22.0",
        "bcryptjs": "^3.0.2",
        "bull": "^4.16.5",
        "cache-manager": "^7.2.4",
        "cache-manager-redis-yet": "^5.1.5",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.2",
        "dotenv": "^17.2.3",
        "ioredis": "^5.8.2",
        "moment": "^2.30.1",
        "ms": "^2.1.3",
        "nodemailer": "^7.0.10",
        "passport": "^0.7.0",
        "passport-google-oauth20": "^2.0.0",
        "passport-jwt": "^4.0.1",
        "passport-local": "^1.0.0",
        "react": "^19.2.0",
        "react-dom": "^19.2.0",
        "redis": "^5.9.0",
        "redis-om": "^0.4.7",
        "reflect-metadata": "^0.2.0",
        "rxjs": "^7.8.1"
    },
    "devDependencies": {
        "@nestjs/cli": "^10.0.0",
        "@nestjs/schematics": "^10.0.0",
        "@nestjs/testing": "^10.0.0",
        "@types/express": "^5.0.0",
        "@types/jest": "^29.5.2",
        "@types/ms": "^2.1.0",
        "@types/multer": "^2.0.0",
        "@types/node": "^20.3.1",
        "@types/passport-google-oauth20": "^2.0.17",
        "@types/passport-jwt": "^4.0.1",
        "@types/passport-local": "^1.0.38",
        "@types/react": "^19.2.2",
        "@types/supertest": "^6.0.0",
        "@typescript-eslint/eslint-plugin": "^8.0.0",
        "@typescript-eslint/parser": "^8.0.0",
        "eslint": "^8.0.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.0",
        "jest": "^29.5.0",
        "prettier": "^3.0.0",
        "prisma": "^6.18.0",
        "source-map-support": "^0.5.21",
        "supertest": "^7.0.0",
        "ts-jest": "^29.1.0",
        "ts-loader": "^9.4.3",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.1.3"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
